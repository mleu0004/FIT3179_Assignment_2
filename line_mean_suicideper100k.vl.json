{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 600,
  "height": 300,
  "title": "Suicides/100k Over Time",
  "data": {
    "url": "https://raw.githubusercontent.com/mleu0004/3179-testrepo/main/suicide.csv"
  },
  "mark": {"type": "line", "point": true},
  "transform": [{"calculate": "year(datum.year)", "as": "Year"}],
  "encoding": {
    "x": {"field": "Year", "type": "nominal"},
    "y": {
      "aggregate": "mean",
      "field": "suicidesPer100k",
      "type": "quantitative",
      "title": "Suicides Per 100k"
    },
    "color": {"value": "teal"},
    "tooltip": [
      {"field": "Year", "type": "nominal"},
      {"field": "sum_suicidesPer100k", "title": "sum suicide rate", "format": "r"}
    ]
  },
  "config": {
    "background":"rgb(239,243,255)"
  }
}